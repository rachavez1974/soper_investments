    <script type="text/javascript">
        var hover = false;
        var m = 0;
        var l = $('.partner-lists').outerWidth();
        var n = l;
        var speed = 20;
        $('#list2').css('left', l);


        $('.partner-lists img').hover(function () {
            var srcs = $(this).attr('src');
            $(this).attr('src', $(this).attr('data-src'));
            $(this).attr('data-src', srcs);
        })


    </script>

    <script>
        var hover = false;
        var m = 0;
        var l = $('.partner-lists').outerWidth();
        var n = l;
        var speed = 20;
        $('#list2').css('left', l);

        function scrollPics() {
            if (hover == false) {
                document.getElementById('list1').style.left = m + 'px';
                document.getElementById('list2').style.left = n + 'px';
                m--;
                n--;
                if (m == -l) {
                    m = l;
                }
                if (n == -l) {
                    n = l;
                }
                //setTimeout(scrollPics(), speed);
            }
        }
        window.onload = function () {
            l = 0;

            $('#list1 li').each(function () {
                l += $(this).outerWidth(true);
                $(this).css('width', $(this).outerWidth(true));

            })
            $('#list2 li').each(function () {
                $(this).css('width', $(this).outerWidth(true));
            })
            n = l;
            $('.partner-lists').css('width', l);
            setInterval(function () {
                scrollPics();
            }, speed);
        }
        $('.partner-lists').hover(function () {
            hover = true;
        }, function () {
            hover = false;
            scrollPics();
        })
        $(window).resize(function (e) {

            e.preventDefault();
            //e.stopPropagation();
            hover = false;
            m = 0;
            l = 0;
            $('#list1 li').removeAttr('style');
            $('#list2 li').removeAttr('style');
            $('.partner-lists').css('width', '100%');
            $('#list1 li').each(function () {
                l += $(this).outerWidth(true);
                $(this).css('width', $(this).outerWidth(true));

            })
            $('#list2 li').each(function () {
                $(this).css('width', $(this).outerWidth(true));
            })
            n = l;
            $('.partner-lists').css('width', l);
            //speed = 20;
            //scrollPics();
        })


        function imgError(image) {

            image.onerror = "";
            image.src = 'https://www.vancestreetcapital.com/images/NoImage.png';
            return true;
        }
    </script>


    <script type="text/javascript">
        $(window).load(function () {
            $('.mobileslides').hide();
            $('.mobileslides').eq(0).addClass('active').show();
            var len = $('.mobileslides').length;
            function callSelf() {
                $('.video-caption h1').html('<span>|</span>');
                var text = $('.mobileslides.active').data('text');
                //console.log(text);



                var appendText = '';
                var i = 0;
                var int = setInterval(function () {
                    appendText += text[i];
                    $('.video-caption h1').html(appendText + '<span>|</span>');
                    i++;
                    if (i == text.length) {
                        clearInterval(int);

                        setTimeout(function () {
                            if ($('.mobileslides.active').index() >= len - 1) {
                                $('.mobileslides.active').fadeOut(500);
                                $('.mobileslides')
                                    .eq(0)
                                    .addClass('active').fadeIn(500);
                                $('.mobileslides.active').eq(1).removeClass('active');
                            } else {
                                $('.mobileslides.active').fadeOut(500);
                                $('.mobileslides.active')
                                    .next('.mobileslides')
                                    .addClass('active').fadeIn(500);
                                $('.mobileslides.active').eq(0).removeClass('active');
                            }
                            callSelf();
                        }, 3000)

                    }
                }, 100)
            }
            callSelf();

            // cursor
                var css = document.createElement("style");
                css.type = "text/css";
               // css.innerHTML = ".video-caption h1 { border-right: 0.08em solid #fff}";
                document.body.appendChild(css);
        })
    </script>




    <script type="text/javascript">
        $(window).load(function () {
            var actionName = 'index';
            var controllerName = 'home';
            actionName = actionName.toLocaleLowerCase();
            controllerName = controllerName.toLocaleLowerCase();;
            try {

                $('.headerlink li').each(function () {
                    $(this).removeClass('active');
                    var $li = $(this);
                    var $link = $(this).find('a');
                    var hrefAction = $link.attr('attrAction').toLocaleLowerCase();
                    var hrefController = $link.attr('attrController').toLocaleLowerCase();
                    if (actionName == 'newsdetail') {
                        actionName = 'index';
                    }
                    if (hrefAction == actionName.toLocaleLowerCase() && hrefController == controllerName.toLocaleLowerCase()) {
                        if (hrefController != 'home') {
                            $li.addClass('active');
                        }

                    }
                });
            } catch (e) {

            }
        });

        //If Image is not Exist then Default Image is shown
        function imgError(image) {
            image.onerror = "";
            image.src = 'https://www.vancestreetcapital.com/images/NoImage.png';
            return true;
        }
    </script>




